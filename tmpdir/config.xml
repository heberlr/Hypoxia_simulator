<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-1500.0</x_min>
		<x_max>1500.0</x_max>
		<y_min>-1500.0</y_min>
		<y_max>1500.0</y_max>
		<z_min>-10.0</z_min>
		<z_max>10.0</z_max>
		<dx>20.0</dx>
		<dy>20.0</dy>
		<dz>20.0</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">600.0</max_time> 
		<time_units>min</time_units>
		<space_units>micron</space_units>	
		
		<dt_diffusion units="min">0.01</dt_diffusion>
		<dt_mechanics units="min">0.1</dt_mechanics>
		<dt_phenotype units="min">6</dt_phenotype>	
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>.</folder>  

		<full_data>
			<interval units="min">60.0</interval>
			<enable>True</enable>
		</full_data>
		
		<SVG>
			<interval units="min">60.0</interval>
			<enable>True</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>

	<microenvironment_setup>
		<variable ID="0" name="oxygen" units="mmHg">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">100000.0</diffusion_coefficient>
				<decay_rate units="1/min">0.1</decay_rate>  
			</physical_parameter_set>
			<initial_condition units="mmHg">45.94</initial_condition>
			<Dirichlet_boundary_condition enabled="true" units="mmHg">45.94</Dirichlet_boundary_condition>
		</variable>
		
		<options>
			<calculate_gradients>True</calculate_gradients>
			<track_internalized_substrates_in_each_agent>False</track_internalized_substrates_in_each_agent>
			 
			<initial_condition enabled="false" type="matlab">
				<filename>./config/initial.mat</filename>
			</initial_condition>
			 
			<dirichlet_nodes enabled="false" type="matlab">
				<filename>./config/dirichlet.mat</filename>
			</dirichlet_nodes>
		</options>
	</microenvironment_setup>
	
	<user_parameters>
	    <tumor_radius type="double" units="micron">250.0</tumor_radius>
		<persitence_timeHip type="double" units="min">3000.0</persitence_timeHip>
		<uptake_rate type="double" units="1/min">10.0</uptake_rate>
		<rate_KnToKp type="double" units="1/min">0.00363</rate_KnToKp> 
		<rate_KpToKn type="double" units="1/min">0.00107</rate_KpToKn> 
		<sigmaS type="double" units="mmHg">38.0</sigmaS>
		<sigmaT type="double" units="mmHg">6.0</sigmaT>
		<speed_red type="double" units="micron/min">0.2938</speed_red> 
		<perst_time_red type="double" units="min">15.0</perst_time_red>
		<bias_red type="double" units="dimensionless">0.1719</bias_red>
		<speed_green type="double" units="micron/min">0.4755</speed_green>
		<perst_time_green type="double" units="min">15.0</perst_time_green>
		<bias_green type="double" units="dimensionless">0.1791</bias_green> 
        <fracResponse_green type="double" units="dimensionless">0.5</fracResponse_green> 
        <bias_greenResp type="double" units="dimensionless">0.5</bias_greenResp>
        <model_stochastic type="bool">False</model_stochastic>
	</user_parameters>
</PhysiCell_settings>